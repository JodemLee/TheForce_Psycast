<Defs>

	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
		<defName>Force_Mental_Training_Targeted</defName>
		<label>Mental Toughness Training(Targeted)</label>
		<description>Using the Force, conduct a focused meditation session to enhance the target's mental resilience. The effect lasts for a few days, depending on your connection to the Force.</description>
		<iconPath>Abilities/JediTraining/MentalTrainingTarget</iconPath>
		<targetMode>Pawn</targetMode>
		<castTime>2400</castTime>
		<durationTime>120000</durationTime>
		<abilityClass>VFECore.Abilities.Ability_Blank</abilityClass>
		<castSound>VPE_Speedboost_Cast</castSound>
		<range>15.9</range>
		<radius>7</radius>
		<hasAoE>true</hasAoE>
		<targetingParametersForAoE>
			<canTargetHumans>True</canTargetHumans>
			<canTargetSelf>false</canTargetSelf>
			<canTargetAnimals>false</canTargetAnimals>
			<neverTargetIncapacitated>True</neverTargetIncapacitated>
		</targetingParametersForAoE>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>Force_JediTraining</path>
				<level>5</level>
				<order>1</order>
				<psyfocusCost>0.12</psyfocusCost>
				<entropyGain>12</entropyGain>
				<psychic>true</psychic>
			</li>
			<li Class="VFECore.Abilities.AbilityExtension_Hediff">
				<hediff>Jedi_Training_Mental</hediff>
				<durationMultiplier>PsychicSensitivity</durationMultiplier>
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>


