	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase">
		<defName>Force_Apprenticeship</defName>
		<label>Apprenticeship</label>
		<description>Take a pawn with a lower psycast level than you as an apprentice. Periodically giving xp. One apprentice at a time.</description>
		<iconPath>Abilities/JediTraining/MasterApprentice</iconPath>
		<abilityClass>TheForce_Psycast.Abilities.Master_Apprentice</abilityClass>
		<targetMode>Pawn</targetMode>
		<range>5</range>
		<targetingParameters>
			<canTargetBuildings>False</canTargetBuildings>
			<canTargetSelf>False</canTargetSelf>
			<canTargetAnimals>False</canTargetAnimals>
			<canTargetPawns>True</canTargetPawns>
			<canTargetHumans>True</canTargetHumans>
			<canTargetMechs>False</canTargetMechs>
		</targetingParameters>
		<castTime>1200</castTime>
		<castSound>VPEP_Puppet_Hit</castSound>
		<modExtensions>
			<li Class="VanillaPsycastsExpanded.AbilityExtension_Psycast">
				<path>VPE_JediTraining</path>
				<level>3</level>
				<order>1</order>
				<spaceAfter>true</spaceAfter>
				<psyfocusCost>0.4</psyfocusCost>
				<entropyGain>0</entropyGain>
				<psychic>true</psychic>
			</li>
		</modExtensions>
	</VFECore.Abilities.AbilityDef>
	
