	<VFECore.Abilities.AbilityDef ParentName="VPE_PsycastBase" Name="Force_LightsaberCombatBase" Abstract="True">
		<requireLineOfSight>True</requireLineOfSight>
		<targetMode>Pawn</targetMode>
